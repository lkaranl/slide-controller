{
  "expo": {
    "name": "Slide Controller",
    "slug": "slide-controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher/res/mipmap-xxxhdpi/ic_launcher.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lnarakl.slidecontroller"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher/web_hi_res_512.png",
        "backgroundColor": "#3f51b5"
      },
      "package": "com.lnarakl.slidecontroller",
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b36cd926-4b23-4b1d-a4fc-40daa17536d1"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "./plugins/withAndroidPermissions"
    ]
  }
}
